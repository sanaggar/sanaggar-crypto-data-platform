# dbt profile for crypto_transform
# Copy to ~/.dbt/profiles.yml or use: dbt run --profiles-dir .
crypto_transform:
  target: dev
  outputs:
    dev:
      type: postgres
      host: localhost
      port: 5432
      user: your_postgres_user
      password: your_postgres_password
      dbname: crypto_data
      schema: public
      threads: 4
